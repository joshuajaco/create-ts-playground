{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "lib": ["ES2019"],
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "target": "ES2019",
    "strict": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  }
}
